<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/grocery_list"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity
            android:name="comp3350.MyGroceryPal.myapplication.NewUI.MainActivityStartScreen.BottomNavBarFragments.LeftButton_Browse.BrowseFragment"
            android:label="@string/title_activity_fragment_browse_v2"
            android:theme="@style/Theme.MyApplication.NoActionBar" />

        <activity
            android:name=".BestStores.Row.ShopOptionRow"
            android:label="@string/title_activity_shop_option_row"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
        <activity
            android:name=".BestStores.BestStoreOption"
            android:label="@string/title_activity_best_store_option"
            android:theme="@style/Theme.MyApplication.NoActionBar" />
        <activity android:name="comp3350.MyGroceryPal.myapplication.NewUI.MainActivityStartScreen.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>